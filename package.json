{
  "name": "portfolio-vera",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "8.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.6.2",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "sass": "^1.56.2",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
